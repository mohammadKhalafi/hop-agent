    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <alt_rec_term/>
    <bad_file/>
    <bind_size>0</bind_size>
    <character_set/>
